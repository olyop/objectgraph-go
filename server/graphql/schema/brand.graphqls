type Brand {
	brandID: UUID!
		@object(key: "Brand/BrandID")
		@cache(ttl: "1d")

	name: String!
		@object(key: "Brand/Name")
		@cache(ttl: "1d")

	updatedAt: Timestamp
		@object(key: "Brand/UpdatedAt")
		@cache(ttl: "1d")

	createdAt: Timestamp!
		@object(key: "Brand/CreatedAt")
		@cache(ttl: "1d")
}

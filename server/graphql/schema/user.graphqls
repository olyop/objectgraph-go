type User {
	userID: UUID! @object(field: "User/UserID")
	userName: String! @object(field: "User/UserName")
	firstName: String! @object(field: "User/FirstName")
	lastName: String! @object(field: "User/LastName")
	dob: Timestamp! @object(field: "User/DOB")
	updatedAt: Timestamp @object(field: "User/UpdatedAt")
	createdAt: Timestamp! @object(field: "User/CreatedAt")

	contacts: [Contact!]! @retrieve(key: "Contact/AllByUserID", args: ["userID=User/UserID"])
}

schema {
	query: Query
	mutation: Mutation
}

scalar UUID
scalar Timestamp

directive @primary on FIELD_DEFINITION | ARGUMENT_DEFINITION
directive @cache(ttl: String!) on FIELD_DEFINITION | OBJECT
directive @object(field: String!) on FIELD_DEFINITION
directive @retrieve(key: String!, args: [String!]) on FIELD_DEFINITION
directive @arg(key: String!) on ARGUMENT_DEFINITION | FIELD_DEFINITION

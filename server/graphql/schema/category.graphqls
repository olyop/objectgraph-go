type Category {
	categoryID: UUID! @object(field: "Category/CategoryID")
	name: String! @object(field: "Category/Name")
	updatedAt: Timestamp @object(field: "Category/UpdatedAt")
	createdAt: Timestamp! @object(field: "Category/CreatedAt")

	classification: Classification! @retrieve(key: "Classification/ByID", args: ["primary=Category/ClassificationID"])
}
